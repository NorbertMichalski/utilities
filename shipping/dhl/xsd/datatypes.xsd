<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.dhl.com/datatypes" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.dhl.com/datatypes" elementFormDefault="unqualified" attributeFormDefault="unqualified">
	<xs:element name="DataTypes">
		<xs:annotation>
			<xs:documentation>Comment describing your root element</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:simpleType name="AccountNumber">
		<xs:annotation>
			<xs:documentation>DHL Account Number</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:maxInclusive value="9999999999"/>
			<xs:minInclusive value="100000000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AccountType">
		<xs:annotation>
			<xs:documentation>Account Type by method of payment ( DHL account vs. Credit card)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<!--Updated by Amruta on 25-Jan-08 for charge card removal requirement-->
			<xs:enumeration value="D"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AddressLine">
		<xs:annotation>
			<xs:documentation>Address Line</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="35" fixed="false"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AdvanceDaysNotice">
		<xs:annotation>
			<xs:documentation>Days of advance notice required</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:nonNegativeInteger">
			<xs:pattern value="\d{0,3}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AWBNumber">
		<xs:annotation>
			<xs:documentation>Airway bill number</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="11"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DHLRoutingCode">
        <xs:annotation>
            <xs:documentation>Routing Code Text</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="InternalServiceCode">
        <xs:annotation>
            <xs:documentation>Handling feature code returned by GLS</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            </xs:restriction>
    </xs:simpleType>
	<xs:simpleType name="BillCode">
		<xs:annotation>
			<xs:documentation>DHL billing options</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="2"/>
			<xs:maxLength value="3"/>
			<xs:enumeration value="DSA"/>
			<xs:enumeration value="DBA"/>
			<!--Commented by Amruta on 25-Jan-08 for charge card removal requirement-->
			<!--xs:enumeration value="CC"/-->
			<xs:enumeration value="TCA"/>
			<xs:enumeration value="IEA"/>
			<xs:enumeration value="UAN"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Billing">
		<xs:sequence>
			<xs:element name="ShipperAccountNumber" type="AccountNumber"/>
			<xs:element name="ShippingPaymentType" type="PaymentType"/>
			<xs:element name="BillingAccountNumber" type="AccountNumber"/>
			<xs:element name="DutyPaymentType" type="DutyTaxPaymentType"/>
			<xs:element name="DutyAccountNumber" type="AccountNumber" minOccurs="0"/>
			<!--Commented by Amruta on 25-Jan-08 for charge card removal requirement-->			
			<!--xs:element name="ChargeCardInfo" type="ChargeCard" minOccurs="0"/-->
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="ChargeCardConfNo">
		<xs:annotation>
			<xs:documentation>Charge card approval number</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:pattern value="\d{0,6}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ChargeCardExpDateValidator">
		<xs:annotation>
			<xs:documentation>Charge card expiration date</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:pattern value="(0[1-9]|1[0-2])\d{1}[0-9]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ChargeCardNo">
		<xs:annotation>
			<xs:documentation>Charge card number</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:minInclusive value="1000000000000"/>
			<xs:maxInclusive value="9999999999999999"/>
			<xs:pattern value="\d{13,16}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ChargeCard">
		<xs:sequence>
			<xs:element name="ChargeCardNo" type="ChargeCardNo"/>
			<xs:element name="ChargeCardType" type="ChargeCardType"/>
			<xs:element name="ChargeCardConfNo" type="ChargeCardConfNo"/>
			<xs:element name="ChargeCardExpiryDate" type="ChargeCardExpDateValidator"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="ChargeCardType">
		<xs:annotation>
			<xs:documentation>Charge card issuer type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
			<xs:enumeration value="AM"/>
			<xs:enumeration value="DC"/>
			<xs:enumeration value="DI"/>
			<xs:enumeration value="MC"/>
			<xs:enumeration value="VI"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="City">
		<xs:annotation>
			<xs:documentation>City name</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="35"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CommodityCode">
		<xs:annotation>
			<xs:documentation>Commodity codes for shipment type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
           <xs:maxLength value="20"/>
        </xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CommodityName">
		<xs:annotation>
			<xs:documentation>Commodity name for shipment content</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="35"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Commodity">
		<xs:sequence>
			<xs:element name="CommodityCode" type="CommodityCode"/>
			<xs:element name="CommodityName" type="CommodityName" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="CommunicationAddress">
		<xs:annotation>
			<xs:documentation>Communications line number: phone number, fax number</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CommunicationType">
		<xs:annotation>
			<xs:documentation>Communications line type (P: phone, F: fax)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
			<xs:enumeration value="P"/>
			<xs:enumeration value="F"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CompanyNameValidator">
		<xs:annotation>
			<xs:documentation>Name of company / business</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="35"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Consignee">
		<xs:sequence>
			<xs:element name="CompanyName" type="CompanyNameValidator" minOccurs="0"/>
			<xs:element name="AddressLine" type="AddressLine" minOccurs="0" maxOccurs="3"/>
			<xs:element name="City" type="City"/>
			<xs:element name="Division" type="Division" minOccurs="0"/>
			<xs:element name="DivisionCode" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PostalCode" type="PostalCode" minOccurs="0"/>
			<xs:element name="CountryCode" type="CountryCode"/>
			<xs:element name="CountryName" type="CountryName"/>
			<xs:element name="FederalTaxId" minOccurs="0">

				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"></xs:minLength>
						<xs:maxLength value="20"></xs:maxLength>
				</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="StateTaxId" minOccurs="0">

				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"></xs:minLength>
						<xs:maxLength value="20"></xs:maxLength>

					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Contact" type="Contact"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Contact">
		<xs:sequence>
			<xs:element name="PersonName" type="PersonName"/>
			<xs:element name="PhoneNumber" type="PhoneNumber"/>
			<xs:element name="PhoneExtension" type="PhoneExtension" minOccurs="0"/>
			<xs:element name="FaxNumber" type="PhoneNumber" minOccurs="0"/>
			<xs:element name="Telex" type="Telex" minOccurs="0"/>
			<xs:element name="Email" type="Email" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="PhoneExtension">
		<xs:restriction base="xs:string">
			<xs:maxLength value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CountryCode">
		<xs:annotation>
			<xs:documentation>ISO country codes</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CountryName">
		<xs:annotation>
			<xs:documentation>ISO country name</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="35"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CourierMsg">
		<xs:annotation>
			<xs:documentation>Courier message for printing on airway bill</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="90"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CurrencyCode">
		<xs:annotation>
			<xs:documentation>ISO currency code</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CurrencyName">
		<xs:annotation>
			<xs:documentation>ISO currency name</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="35"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DateTime">
		<xs:annotation>
			<xs:documentation>Date and time</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="Date">
		<xs:annotation>
			<xs:documentation>Date only</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:date"/>
	</xs:simpleType>
	<xs:simpleType name="DayHour">
		<xs:annotation>
			<xs:documentation>Day and hour only</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="(0[1-9]|1[0-9]|2[0-9]|3[0-1]) ([01-24])"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DoorTo">
		<xs:annotation>
			<xs:documentation>Defines the type of delivery service that applies to the shipment</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
			<xs:enumeration value="DD"/>
			<xs:enumeration value="DA"/>
			<xs:enumeration value="AA"/>
			<xs:enumeration value="DC"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Dutiable">
		<xs:sequence>
			<xs:element name="DeclaredValue" type="Money" minOccurs="0" />
			<xs:element name="DeclaredCurrency" type="CurrencyCode" minOccurs="0" />
			<xs:element name="ScheduleB" type="ScheduleB" minOccurs="0" />
			<xs:element name="ExportLicense" minOccurs="0" />
			<xs:element name="ShipperEIN" minOccurs="0" >
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ShipperIDType" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="S" />
						<xs:enumeration value="E" />
						<xs:enumeration value="D" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ConsigneeIDType" minOccurs="0">

				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:length value="1"></xs:length>
						<xs:enumeration value="S" />
						<xs:enumeration value="E" />
						<xs:enumeration value="D" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ImportLicense" minOccurs="0" />
			<xs:element name="ConsigneeEIN" minOccurs="0">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation>
							Consigneeâ€™s tax ID or VAT number.
						</xs:documentation>
					</xs:annotation>
				<!--<xs:element name="ConsigneeIDType" minOccurs="0">-->
				
						<xs:restriction base="xs:string">
							<xs:maxLength value="20"/>
						</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TermsOfTrade" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="DutyTaxPaymentType">
		<xs:annotation>
			<xs:documentation>Duty and tax charge payment type (S:Shipper, R:Recipient, T:Third Party/Other)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
			<xs:enumeration value="S"/>
			<xs:enumeration value="R"/>
			<xs:enumeration value="T"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DivisionCode">
		<xs:annotation>
			<xs:documentation>Division (e.g. state, prefecture, etc.) code</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Division">
		<xs:annotation>
			<xs:documentation>Division (e.g. state, prefecture, etc.) name</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="35"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="EmailBody">
		<xs:annotation>
			<xs:documentation>Body of an email message</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="255"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Email">
		<xs:annotation>
			<xs:documentation>Email message</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="From" type="EmailAddress"/>
			<xs:element name="To" type="EmailAddress"/>
			<xs:element name="cc" type="EmailAddress" minOccurs="0" maxOccurs="5"/>
			<xs:element name="Subject" type="xs:string" minOccurs="0"/>
			<xs:element name="ReplyTo" type="EmailAddress" minOccurs="0"/>
			<xs:element name="Body" type="EmailBody" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="EmailAddress">
		<xs:annotation>
			<xs:documentation>Email address containing '@'</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:complexType name="ExportDeclaration">
		<xs:sequence>
			<xs:element name="InterConsignee" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="70"/>
						<xs:minLength value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="IsPartiesRelation" type="YesNo" minOccurs="0"/>
			<xs:element name="ECCN" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="11"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="SignatureName" type="SignatureName" minOccurs="0"/>
			<xs:element name="SignatureTitle" type="SignatureTitle" minOccurs="0"/>
			<xs:element name="ExportReason" type="ExportReason" minOccurs="0"/>
			<xs:element name="ExportReasonCode" type="ExportReasonCode" minOccurs="0"/>
			<xs:element name="SedNumber" type="SEDNumber" minOccurs="0"/>
			<xs:element name="SedNumberType" type="SEDNumberType" minOccurs="0"/>
			<xs:element name="MxStateCode" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ExportLineItem" type="ExportLineItem" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>specifics  about  each of the line item</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ExportLineItem">
		<xs:sequence>
			<xs:element name="LineNumber" type="LineNumber"/>
			<xs:element name="Quantity" type="Quantity"/>
			<xs:element name="QuantityUnit" type="QuantityUnit"/>
			<xs:element name="Description" type="xs:string"/>
			<xs:element name="Value" type="Money"/>
			<xs:element name="IsDomestic" type="YesNo" minOccurs="0"/>
			<xs:element name="CommodityCode" type="CommodityCode" minOccurs="0"/>
			<xs:element name="ScheduleB" type="ScheduleB" minOccurs="0"/>
			<xs:element name="ECCN" type="xs:string" minOccurs="0"/>
			<xs:element name="Weight" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Weight" type="Weight"/>
						<xs:element name="WeightUnit" type="WeightUnit"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="License" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="LicenseNumber" type="LicenseNumber">
							<xs:annotation>
								<xs:documentation>shipper export license</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ExpiryDate" type="xs:date"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="LicenseSymbol" type="LicenseNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>license excption symbol</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="SignatureName">
		<xs:annotation>
			<xs:documentation>Signature name </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="35"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SignatureTitle">
		<xs:annotation>
			<xs:documentation>Signature title </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="35"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ExportReasonCode">
		<xs:annotation>
			<xs:documentation>Export reason code (P:Permanent, T:Temporary, R:Re-Export)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
			<xs:enumeration value="P"/>
			<xs:enumeration value="T"/>
			<xs:enumeration value="R"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="GlobalProductCode">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
            <xs:maxLength value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ExportReason">
		<xs:annotation>
			<xs:documentation>Export reason</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IDType">
		<xs:annotation>
			<xs:documentation>ID Type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="SSN"/>
			<xs:enumeration value="EIN"/>
			<xs:enumeration value="DUNS"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PiecesEnabled">
        <xs:annotation>
            <xs:documentation>Pieces Enabling Flag</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Y"/>
            <xs:enumeration value="N"/>
        </xs:restriction>
    </xs:simpleType>
	<xs:simpleType name="LanguageCode">
		<xs:annotation>
			<xs:documentation>ISO Language Code</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="LicenseNumber">
		<xs:annotation>
			<xs:documentation>Export license number</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="16"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LineNumber">
		<xs:restriction base="xs:positiveInteger">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="200"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LocalProductCode">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
            <xs:maxLength value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Money">
		<xs:annotation>
			<xs:documentation>Monetary amount (with 2 decimal precision)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:float">
			<xs:minInclusive value="0.00"/>
			<xs:maxInclusive value="9999999999.99"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PackageType">
		<xs:annotation>
			<xs:documentation>Package Type (EE: DHL Express Envelope, OD:Other DHL Packaging, CP:Customer-provided.Ground shipments must choose CP)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
			<xs:enumeration value="EE"/>
